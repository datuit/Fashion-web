generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model sessions {
  data       String?
  expires    Int
  session_id String  @id
}

model users {
  avatarUrl   String?
  create_time DateTime?
  email       String?
  facebookId  String?
  fullname    String?
  id          Int       @default(autoincrement()) @id
  password    String
  username    String    @unique
}